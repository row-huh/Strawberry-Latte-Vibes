{
    "name": "StrawberryLatteVibes",
    "colors": {
        "editor.background": "#ecddcf",  
        "editor.foreground": "#3d301e",  
        "list.activeSelectionIconForeground": "#D2B48C" ,
		"sideBar.background": "#ecddcf", 
        "sideBar.foreground": "#DE3163", 
        "sideBarSectionHeader.background": "#E8E0D9", 
        "sideBarSectionHeader.foreground": "#6B4E3D", 
        "sideBarTitle.foreground": "#4A3B31", 
        "list.hoverBackground": "#F0E6D2", 
        "list.inactiveSelectionBackground": "#E8E0D9", 
        "list.inactiveSelectionForeground": "#4A3B31", 
        "list.activeSelectionBackground": "#D2B48C33", 
        "list.activeSelectionForeground": "#4A3B31", 
        "list.focusBackground": "#D2B48C66",
		"tab.activeBackground": "#ecddcf",
		"tab.inactiveBackground" : "#d1996457",
		"tab.hoverBackground" : "#ecddcf",
        "tab.activeBorder": "#483D8B",
		// bottom panel colors
		"panel.background": "#ecddcf",
        "panel.border" : "#483D8B",

		"statusBar.background" :  "#DE3163",
		"activityBar.background" : "#DE3163",
		"activityBar.foreground" : "#ffffff",

		// header colors
		"editorGroupHeader.tabsBackground": "#ecddcf",
		"titleBar.activeBackground": "#DE3163",
        "titleBar.border": "#483D8B"
    },

    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
				"foreground" : "#00A7B3"  
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#A0785A"  
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#556B2F" 
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#8B0000" 
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#5A5A5A" 
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#DE3163" 
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#8A00C4" 
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#CD853F"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#483D8B"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#483D8B" 
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9E3A26" 
            }
        },
        {
            "name": "HTML: Entities",
            "scope": [
                "constant.character.entity",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#CD853F" 
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#483D8B" 
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#E40078" 
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#556B2F"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#A0522D" 
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#8B0000" 
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#8B0000" 
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#228B22" 
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#6A5ACD" 
            }
        },
        {
            "name": "Markup: Output",
            "scope": [
                "markup.output",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#5A5A5A" 
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#5A5A5A" 
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#8B4513" 
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#8B0000" 
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#483D8B" 
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#6A5ACD" 
            }
        },
        {
            "name": "Markup Styling",
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#556B2F" 
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#CD853F" 
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": [
                "meta.diff.range",
                "meta.diff.index",
                "meta.separator"
            ],
            "settings": {
                "foreground": "#696969" 
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": "meta.diff.header.from-file",
            "settings": {
                "foreground": "#696969" 
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": "meta.diff.header.to-file",
            "settings": {
                "foreground": "#696969"
            }
        }
    ]
}
